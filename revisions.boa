# list of files modified per commit (impactful commits)

p: Project = input;

out : output collection[string] of string;

fileslist: string = "";

visit(p, visitor {
  before rev: Revision -> {
    if(len(rev.files) < 5){
      foreach(i:int; rev.files[i]){
        fileslist = fileslist+rev.files[i].name;
          fileslist = fileslist + ",";
        }
        out[formattime("%c", rev.commit_date)] << fileslist;
        fileslist = " ";
   }
 }
 });
